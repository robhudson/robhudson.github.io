{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all clean --parallel build:css build:html",
    "build:css": "postcss src/css/index.css -o _site/css/index.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "postcss src/css/index.css -o _site/css/index.css --watch",
    "watch:html": "eleventy --serve",
    "start": "npm run watch",
    "clean": "rm -rf ./_site"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-plugin-rss": "1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.4",
    "@tailwindcss/typography": "0.3.1",
    "autoprefixer": "10.0.2",
    "eleventy-plugin-reading-time": "0.0.1",
    "luxon": "2.5.2",
    "markdown-it": "12.0.2",
    "markdown-it-anchor": "6.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.1.8",
    "postcss-clean": "1.1.0",
    "postcss-cli": "8.3.0",
    "tailwindcss": "2.0.1"
  }
}
